spatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(fetchComments(props.quoteId))\n    return () => {\n      dispatch(commentsActions.clearComments());\n      dispatch(commentsActions.setLoadingStart());\n    }\n  }, [dispatch]);\n\n  const startAddCommentHandler = () => {\n    setIsAddingComment(true);\n  };\n\n  const commentContent =  <section className={classes.comments}>\n    <h2>User Comments</h2>\n      {comments.lenght > 0} {\n        <CommentsList comments={comments} />\n      }\n      {comments.length === 0 && !isAddingComment && <h4>No comments added yet</h4>}\n      {!isAddingComment && (\n        <button className=\"btn\" onClick={startAddCommentHandler}>\n          Add a Comment\n        </button>\n      )}\n      {isAddingComment && <NewCommentForm />}\n      \n    </section>;\n\n  return <>\n    {isLoading && <div className=\"centered\"> <LoadingSpinner /> </div>}\n    {!isLoading  && commentContent}    \n    </>;\n};\n\nexport default Comments;\n","S:\\Personal Files\\Coding\\Websites\\React\\React - The Complete Guide [Revised, Summer 2021]\\20 React Router\\quotes-project\\src\\components\\comments\\NewCommentForm.js",[],"S:\\Personal Files\\Coding\\Websites\\React\\React - The Complete Guide [Revised, Summer 2021]\\20 React Router\\quotes-project\\src\\components\\layout\\Layout.js",[],"S:\\Personal Files\\Coding\\Websites\\React\\React - The Complete Guide [Revised, Summer 2021]\\20 React Router\\quotes-project\\src\\components\\layout\\MainNavigation.js",[],"S:\\Personal Files\\Coding\\Websites\\React\\React - The Complete Guide [Revised, Summer 2021]\\20 React Router\\quotes-project\\src\\components\\quotes\\QuoteList.js",[],"S:\\Personal Files\\Coding\\Websites\\React\\React - The Complete Guide [Revised, Summer 2021]\\20 React Router\\quotes-project\\src\\components\\quotes\\QuoteItem.js",[],"S:\\Personal Files\\Coding\\Websites\\React\\React - The Complete Guide [Revised, Summer 2021]\\20 React Router\\quotes-project\\src\\componen